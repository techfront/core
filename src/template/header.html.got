                    <input class="input input--theme-search" name="q" type="text" placeholder="Что найти...">
                </form>
                <a class="js-show button button--theme-icon button--theme-primary-darkness header__search-close" href="#" data-show=".js-show-target--search"><i
                        class="icon-cancel"></i></a>
            </div>
