        <div class="js-show-target--menu header__menu header__menu--hidden">
            <nav class="menu" alt="Techfront - Новости">
                <ul class="menu__list">
                    <li class='menu__item {{ if eq .current_path "/" }}menu__item--active{{ end }}'>
                        <a class="menu__item-link" href="/">Лучшее</a>
                    </li>
                    <li class='menu__item {{ if eq .current_path "/topics" }}menu__item--active{{ end }}'>
                        {{ if not (eq .topic_newest_count 0) }}
                            <a class="menu__item-link" title="Новые топики: {{ .topic_newest_count }} за сегодня" href="/topics">Новое
                            <span class="menu__item-newest-count">{{ .topic_newest_count }}</span></a>
                        {{ else }}
                            <a class="menu__item-link" href="/topics">Новое</a>
                        {{ end }}
                    </li>
                    <li class='menu__item {{ if eq .current_path "/users" }}menu__item--active{{ end }}'>
                        <a class="menu__item-link" href="/users">Сообщество</a>
                    </li>
                    <li class='menu__item {{ if eq .current_path "/discussions" }}menu__item--active{{ end }}'>
                        <a class="menu__item-link" href="/discussions">Дискуссии</a>
                    </li>
                    <li class='menu__item {{ if eq .current_path "/underground" }}menu__item--active{{ end }}'>
                        {{ if not (eq .offer_newest_count 0) }}
                            <a class="menu__item-link" title="Новые офферы: {{ .offer_newest_count }} за сегодня" href="/underground">Андеграунд
                            <span class="menu__item-newest-count">{{ .offer_newest_count }}</span></a>
                        {{ else }}
                            <a class="menu__item-link" href="/underground">Андеграунд</a>
                        {{ end }}
                    </li>

