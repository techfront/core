                <a href="#" class="js-dropdown dropdown__toggle header__username-link" data-menu="#dropdown_id_0">{{.current_user.Name}}</a>
                <ul id="dropdown_id_0" class="dropdown__menu">
                    <li class="dropdown__item"><a class="dropdown__item-link" href="/users/{{.current_user.Id}}"><i class="icon-user-secret"></i> Профиль</a></li>
                    <li class="dropdown__item"><a class="dropdown__item-link" href="/favorited"><i class="icon-bookmark"></i> Избранное</a></li>
                    <li class="dropdown__item"><a class="dropdown__item-link" href="/users/{{.current_user.Id}}/update"><i class="icon-wrench"></i> Настройки</a></li>
