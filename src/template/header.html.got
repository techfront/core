            {{ if not .current_user.Anon }}
            <a class="header__action-button button" href="/submit/topic" title="Создать топик"><i
                    class="icon-pencil-1"></i> <span class="header__action-text">Создать топик</span></a>
            {{ else }}
            <a class="js-modal-show header__action-button button" href="/signup" data-modal=".js-modal-target--user-create"><i class="icon-key"></i> <span class="header__action-text">Начать обсуждение</span></a>
            {{ end }}
        </div>
