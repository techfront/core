                      {{ end }}

                      <p class="digest__item-heading" style="margin-top: 0;margin-bottom: 18px;color: #7f8c8d;font-size: 16px;line-height: 1.5em;margin: 5px 0;padding: 0;font-family: -apple-system,system-ui,BlinkMacSystemFont,&quot;Segoe UI&quot;,Roboto,&quot;Helvetica Neue&quot;,Arial,sans-serif;-webkit-box-sizing: border-box;box-sizing: border-box;"><a href="{{ .RSSDestinationURL }}" target="_blank" class="digest__item-heading-link" style="color: #000;text-decoration: none;font-weight: 600;font-size: 18px;font-family: -apple-system,system-ui,BlinkMacSystemFont,&quot;Segoe UI&quot;,Roboto,&quot;Helvetica Neue&quot;,Arial,sans-serif;-webkit-box-sizing: border-box;box-sizing: border-box;">{{ .Name }}</a></p>
                      <div class="digest__item-meta" style="margin: 10px 0 5px;font-family: -apple-system,system-ui,BlinkMacSystemFont,&quot;Segoe UI&quot;,Roboto,&quot;Helvetica Neue&quot;,Arial,sans-serif;-webkit-box-sizing: border-box;box-sizing: border-box;">
