                      {{ else if .IsQuestion }}
                          <a href="https://techfront.org/format/question" class="digest__item-format digest__item-format--question" style="color: #FFF;text-decoration: none;display: inline-block;background: #27ae60;padding: 4px 12px;line-height: 1;vertical-align: baseline;font-size: 13px;border-radius: 3px;margin: 5px 0;font-family: -apple-system,system-ui,BlinkMacSystemFont,&quot;Segoe UI&quot;,Roboto,&quot;Helvetica Neue&quot;,Arial,sans-serif;-webkit-box-sizing: border-box;box-sizing: border-box;">Вопрос</a>
