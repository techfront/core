                      {{ else if .IsNews }}
                          <a href="https://techfront.org/format/news" class="digest__item-format digest__item-format--news" style="color: #FFF;text-decoration: none;display: inline-block;background: #34495e;padding: 4px 12px;line-height: 1;vertical-align: baseline;font-size: 13px;border-radius: 3px;margin: 5px 0;font-family: -apple-system,system-ui,BlinkMacSystemFont,&quot;Segoe UI&quot;,Roboto,&quot;Helvetica Neue&quot;,Arial,sans-serif;-webkit-box-sizing: border-box;box-sizing: border-box;">Новость</a>
                      {{ else if .IsVideo }}
